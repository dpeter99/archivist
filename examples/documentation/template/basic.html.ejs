<!DOCTYPE html>
<html lang="en">
<head>
    <link rel="stylesheet" href="/style.css">
</head>
<body>

<div class="page">
    <div class="content">

        <h1 id="{{id}}" data-uid="{{uid}}" class="text-break"><%- meta.name %></h1>
        <div class="markdown level0 summary"><%- helper.md(meta["code:data"].desc) %></div>


        <% meta["code:data"].members.forEach(member => { %>
                <div class="member">
                    <div class="member-header">
                    <div class="kind">
                        <%- helper.md(member.kind) %>
                    </div>
                    <div class="visibility">
                        <%- helper.md(member.visibility)%>
                    </div>
                    <div class="member__type">
                        <%- helper.md(member.type) %>
                    </div>
                    <div class="member__name">
                        <%- member.name %>
                    </div>
                    </div>
                    <div class="member-description">
                        <%- helper.md(member.brief)%>
                    </div>
                </div>
        <% });%>

    </div>
</div>

</body>

</html>
